{
  "name": "typescript-webapi-inversify",
  "version": "1.0.0",
  "description": "Starter project with TypeScript, Inversify, Swagger and Jasmine",
  "scripts": {
    "dev": "ts-node ./src/server.ts",
    "test": "node spec/spec.js",
    "test-cover": "istanbul cover spec/spec.js  && istanbul check-coverage",
    "test-nyc": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json --reporter=jasmine-console-reporter && istanbul check-coverage",    
    "serve": "tsc && node ./dist/src/server.js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false  
  },
  "author": "Daniel Edreira",
  "license": "Apache-2.0",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/jasmine": "^3.4.4",
    "istanbul": "^0.4.5",
    "jasmine": "^3.5.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-node": "^3.0.0",
    "nyc": "^14.1.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.13",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.12",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "inversify-logger-middleware": "3.1.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "jasmine": "^3.5.0",
    "jasmine-node": "^3.0.0",    
    "request": "^2.88.0",
    "swagger-express-ts": "^1.0.1",
    "swagger-ui-dist": "^3.24.0",
    "tslib": "^1.10.0"
  }
}
